{
  "name": "am2h-core-app",
  "version": "0.9.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:electron": "ng build && electron .",
    "build": "ng build",
    "build:windows": "ng build && electron-builder --win --dir",
    "dist:windows": "ng build && electron-builder --win",
    "build:linux": "ng build && electron-builder --linux --dir",
    "dist:linux": "ng build && electron-builder --linux",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "build": {
    "appId": "am2h-core-app",
    "linux": {
      "category": "Utility"
    },
    "files": [
      "dist/am2h-core-app/*",
      "index.js"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.4",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^17.2.4",
    "@angular/compiler": "^17.2.4",
    "@angular/core": "^17.2.4",
    "@angular/forms": "^17.2.4",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^17.2.4",
    "@angular/platform-browser-dynamic": "^17.2.4",
    "@angular/router": "^17.2.4",
    "@influxdata/influxdb-client": "^1.32.0",
    "@types/ws": "^8.5.10",
    "mqtt": "^4.3.7",
    "ngx-mqtt": "^17.0.0",
    "npm": "^9.1.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular/cli": "^17.2.3",
    "@angular/compiler-cli": "^17.2.4",
    "@types/jasmine": "~3.10.0",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.3.3"
  }
}