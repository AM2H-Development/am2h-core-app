{
  "name": "am2h-core-app",
  "version": "0.9.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:electron": "ng build && electron .",
    "build": "ng build",
    "build:windows": "ng build && electron-builder --win --dir",
    "dist:windows": "ng build && electron-builder --win",
    "build:linux": "ng build && electron-builder --linux --dir",
    "dist:linux": "ng build && electron-builder --linux",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "build": {
    "appId": "am2h-core-app",
    "linux": {
      "category": "Utility"
    },
    "files": [
      "dist/am2h-core-app/*",
      "index.js"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.1.4",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.1.4",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@influxdata/influxdb-client": "^1.32.0",
    "mqtt": "^4.3.7",
    "ngx-mqtt": "^9.0.5",
    "npm": "^9.1.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.9.5"
  }
}